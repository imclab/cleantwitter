{
  "name": "cleantwitter",
  "version": "1.0",
"update_url": "http://github.com/urschrei/cleantwitter/blob/master/chrome/updates.xml",
  "description": "Removes irritating social features from twitter.com",
  "permissions": ["http://twitter.com/*", "https://twitter.com/*"],
	"content_scripts": [
    {
      "matches": ["http://twitter.com/*", "https://twitter.com/*"],
      "js" : ["chrome_script.js"]
    }
]
}